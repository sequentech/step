<div style="margin: 0 auto; background-color: #ffffff">
      <!-- MAIN section -->
      <table align="center" style="width: 100%">
        <tr>
          <td style="text-align: center; padding: 10px 0">
            <div style="width: 100%">
              <!-- Inner table section -->
              <table style="width: 100%">
                <!-- End of report section -->
                <tr>
                  <td align="left">
                    <table style="width: 100%">
                      <tr>
                        <td align="left">
                          <div
                            style="
                              text-align: left;
                              margin: 16px 0;
                              font-size: 10px;
                            "
                          >
                            <p>{{datetime from_rfc3339=date_printed output_format="%B %d, %Y %H:%M"}}</p>
                          </div>
                        </td>
                        <td align="right">
                          <div
                            style="
                              text-align: right;
                              margin: 16px 0;
                              font-size: 10px;
                            "
                          ></div>
                        </td>
                      </tr>
                    </table>
                  </td>
                </tr>
                <!-- End of report section -->

                <!-- Document header block -->
                <tr>
                  <td>
                    <table style="width: 100%">
                      <td width="25%">
                        <img
                          style="display: inline-block"
                          src=""
                          style="max-width: 80px"
                        />
                      </td>
                      <td width="50%" align="center" style="font-family: serif">
                        REPUBLIC OF PHILIPPINES
                        <br /><strong>COMMISSION ON ELECTIONS</strong>
                        <br />{{datetime from_rfc3339=election_date output_format="%B %d, %Y"}} <br />{{election_title}}
                      </td>
                      <td width="25%">
                        <img src="" style="max-width: 80px" />
                      </td>
                    </table>
                  </td>
                </tr>
                <!-- Document header block -->

                <!-- Subtitle header block -->
                <tr>
                  <td
                    align="center"
                    style="font-family: serif; font-size: 12px"
                  >
                    <div
                      style="
                        margin-top: 12px;
                        margin-bottom: 12px;
                        text-align: center;
                      "
                    >
                      <strong>ONLINE VOTING AND COUNTING SYSTEM</strong>
                      <br />Overseas Voting Period: {{voting_period}}
                    </div>
                  </td>
                </tr>
                <!-- Subtitle header block -->

                <!-- Main title -->
                <tr>
                  <td
                    align="center"
                    style="font-family: serif; font-size: 18px"
                  >
                    <p style="margin-bottom: 0">
                      <strong>OVERSEAS VOTING MONITORING</strong>
                    </p>
                    <p style="margin: 0">OVCs Statistics</p>
                  </td>
                </tr>
                <!-- Main title -->

                <!-- Additional sections -->
                <tr>
                  <td style="margin: 16px 0">
                    <div style="margin: 16px 0">
                      <table style="width: 100%; border: 1px solid black">
                        <thead>
                          <tr>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>Region/Post</strong>
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>Country</strong>
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>No. of ROVs</strong>
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong
                                >No. of OV who did not pre-enrolled</strong
                              >
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>No. of OV who pre-enrolled</strong>
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong
                                >No. of OV pre-enrroled but did not vote</strong
                              >
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong
                                >No. of OV who pre-enrolled but voted</strong
                              >
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>No. of OV who actually voted</strong>
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong
                                >No. of OV with password reset request</strong
                              >
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>Remarks</strong>
                            </td>
                          </tr>
                        </thead>
                        <tbody>
                          {{#each regions}}
                          <tr>
                            <td
                              colspan="9"
                              style="
                                text-align: left;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>{{name}}</strong>
                            </td>
                          </tr>

                          {{#each data}}
                          <tr>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{post}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{country}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{total}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{not_pre_enrolled}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{pre_enrolled}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{pre_enrolled_not_voted}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{pre_enrolled_voted}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{voted}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{password_reset_request}}
                            </td>
                            <td
                              style="
                                text-align: left;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{remarks}}
                            </td>
                          </tr>
                          {{/each}} {{/each}}
                        </tbody>
                      </table>
                    </div>
                  </td>
                </tr>
                <!-- END of additional sections -->

                <!-- Additional sections -->
                <tr>
                  <td style="margin: 16px 0">
                    <div style="margin: 16px 0">
                      <table style="width: 100%">
                        <tr>
                          <td style="text-align: left; vertical-align: top">
                            <strong>No. of OV disapproved by OFOV:</strong>
                          </td>
                          <td style="text-align: left">{{ofov_disapproved}}</td>
                        </tr>
                        <tr>
                          <td style="text-align: left; vertical-align: top">
                            <strong>No. of OV disapproved by SBEI:</strong>
                          </td>
                          <td style="text-align: left">{{sbei_disapproved}}</td>
                        </tr>
                        <tr>
                          <td style="text-align: left; vertical-align: top">
                            <strong>No. of OV disapproved by System:</strong>
                          </td>
                          <td style="text-align: left">
                            {{system_disapproved}}
                          </td>
                        </tr>
                      </table>
                    </div>
                  </td>
                </tr>
                <!-- END of additional sections -->

                <!-- Title secondary section -->
                <tr>
                  <td align="center" style="padding: 15px">
                    <div style="text-align: center; padding: 15px">
                      {{#each qr_codes}}
                      <img src="{{this}}" max-width="80" />
                      {{/each}}
                    </div>
                  </td>
                </tr>
                <!-- END Title secondary section -->

                <!-- Left paragraph section -->
                <tr>
                  <td align="center" style="padding: 10px">
                    <div style="text-align: left; margin-top: 24px">
                      Report hash: {{report_hash}}<br />
                      Software version: {{software_version}}<br />
                      OVCS version: {{ovcs_version}}<br />
                      System hash: {{system_hash}}
                    </div>
                  </td>
                </tr>
                <!-- END Left paragraph section -->

                <!-- End of report section -->
                <tr>
                  <td align="center">
                    <div style="text-align:center; margin: 16px; 0">
                      <hr
                        style="margin: 0 24px; border-top: 3px solid #000000"
                      />
                      <p>END OF REPORT</p>
                    </div>
                  </td>
                </tr>
                <!-- End of report section -->
              </table>
              <!-- Inner table section -->
            </div>
          </td>
        </tr>
      </table>
      <!-- MAIN section -->
</div>

<div style="margin: 0 auto; background-color: #ffffff">
      <!-- MAIN section -->
      <table align="center" style="width: 100%">
        <tr>
          <td style="text-align: center; padding: 10px 0">
            <div style="width: 100%">
              <!-- Inner table section -->
              <table style="width: 100%">
                <!-- End of report section -->
                <tr>
                  <td align="left">
                    <table style="width: 100%">
                      <tr>
                        <td align="left">
                          <div
                            style="
                              text-align: left;
                              margin: 16px 0;
                              font-size: 10px;
                            "
                          >
                            <p>{{datetime from_rfc3339=date_printed output_format="%B %d, %Y %H:%M"}}</p>
                          </div>
                        </td>
                        <td align="right">
                          <div
                            style="
                              text-align: right;
                              margin: 16px 0;
                              font-size: 10px;
                            "
                          ></div>
                        </td>
                      </tr>
                    </table>
                  </td>
                </tr>
                <!-- End of report section -->

                <!-- Document header block -->
                <tr>
                  <td>
                    <table style="width: 100%">
                      <td width="25%" style="text-align: left">
                        <img
                          width="140px"
                          style="display: inline-block"
                          
                        />
                      </td>
                    </table>
                  </td>
                </tr>
                <!-- Document header block -->

                <!-- Subtitle header block -->
                <tr>
                  <td
                    align="center"
                    style="font-family: serif; font-size: 12px"
                  >
                    <div
                      style="
                        margin-top: 12px;
                        margin-bottom: 12px;
                        text-align: center;
                      "
                    >
                      <strong>ONLINE VOTING AND COUNTING SYSTEM</strong>
                      <br />Overseas Voting Period: {{voting_period}}
                    </div>
                  </td>
                </tr>
                <!-- Subtitle header block -->

                <!-- Main itle -->
                <tr>
                  <td
                    align="center"
                    style="font-family: serif; font-size: 18px"
                  >
                    <p>
                      <strong>LIST OF OVERSEAS VOTERS</strong>
                    </p>
                  </td>
                </tr>
                <!-- Main itle -->

                <!-- Additional sections -->
                <tr>
                  <td style="margin: 16px 0">
                    <div style="margin: 16px 0">
                      <table style="width: 100%">
                        <tr>
                          <td style="text-align: left; vertical-align: top">
                            <strong>Post:</strong>
                          </td>
                          <td style="text-align: left">{{post}}</td>
                        </tr>
                        <tr>
                          <td style="text-align: left; vertical-align: top">
                            <strong>Country:</strong>
                          </td>
                          <td style="text-align: left">{{country}}</td>
                        </tr>
                      </table>
                    </div>
                  </td>
                </tr>
                <!-- END of additional sections -->

                <!-- Additional sections -->
                <tr>
                  <td style="margin: 16px 0">
                    <div style="margin: 16px 0">
                      <table style="width: 100%; border: 1px solid black">
                        <thead>
                          <tr>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>No.</strong>
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>Name</strong>
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>Status</strong>
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>Date voted</strong>
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              <strong>Time voted</strong>
                            </td>
                          </tr>
                        </thead>
                        <tbody>
                          {{#each voters}}
                          <tr>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{number}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{last_name}},
                              {{first_name}}{{suffix}}{{middle_name}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{status}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{format_date date_voted "%B %d, %Y"}}
                            </td>
                            <td
                              style="
                                text-align: center;
                                vertical-align: top;
                                border: 1px solid black;
                              "
                            >
                              {{time_voted}}
                            </td>
                          </tr>
                          {{/each}}
                        </tbody>
                      </table>
                    </div>
                  </td>
                </tr>
                <!-- END of additional sections -->

                <!-- Additional sections -->
                <tr>
                  <td style="margin: 16px 0">
                    <div style="margin: 16px 0">
                      <table style="width: 100%">
                        <tr>
                          <td style="text-align: left; vertical-align: top">
                            <strong>No. of OV who voted:</strong>
                          </td>
                          <td style="text-align: left">{{ov_voted}}</td>
                        </tr>
                        <tr>
                          <td style="text-align: left; vertical-align: top">
                            <strong>No. of OV who did not vote:</strong>
                          </td>
                          <td style="text-align: left">{{ov_not_voted}}</td>
                        </tr>
                        <tr>
                          <td style="text-align: left; vertical-align: top">
                            <strong>No. of OV who did not pre-enroll:</strong>
                          </td>
                          <td style="text-align: left">
                            {{ov_not_pre_enrolled}}
                          </td>
                        </tr>
                        <tr>
                          <td style="text-align: left; vertical-align: top">
                            <strong
                              >No. of EBs with Voting Privilege who
                              voted:</strong
                            >
                          </td>
                          <td style="text-align: left">{{eb_voted}}</td>
                        </tr>
                        <tr>
                          <td style="text-align: left; vertical-align: top">
                            <strong>Total No. of OVs:</strong>
                          </td>
                          <td style="text-align: left">{{ov_total}}</td>
                        </tr>
                      </table>
                    </div>
                  </td>
                </tr>
                <!-- END of additional sections -->

                <!-- Paragraph section -->
                <tr>
                  <td align="left">
                    <div
                      style="
                        margin-top: 12px;
                        margin-bottom: 12px;
                        text-align: left;
                      "
                    >
                      <strong
                        >WE, THE UNDERSIGNED MEMBERS OF THE SPECIAL BOARD OF
                        ELECTION INSPECTORS</strong
                      >

                      of {{post}}, do hereby certify under oath that we
                      witnessed the voting of the Clustered Precinct ID
                      {{precinct_code}}, and that the votes obtained by each
                      candidate appearing in the
                      <strong>ELECTION RETURNS FOR NATIONAL POSITIONS</strong>
                      are true as generated by the Online Voting and Counting
                      System for {{country }} on {{goverment_time}} (hostâ€™s
                      government time) ({{local_time}} in the Philippines).
                    </div>
                  </td>
                </tr>
                <!-- END Paragraph section -->

                <!-- Title secondary section -->
                <tr>
                  <td align="center" style="padding: 15px">
                    <div style="text-align: center; padding: 15px">
                      <p style="font-family: serif; font-size: 12pt">
                        <strong
                          >THE SPECIAL BOARD OF ELECTION INSPECTORS</strong
                        >
                      </p>
                    </div>
                  </td>
                </tr>
                <!-- END Title secondary section -->

                <!-- Signature simple section -->
                <tr>
                  <td align="center" style="margin-top: 24px">
                    <div style="margin-top: 24px">
                      <hr
                        style="margin: 0 120px; border-top: 1px solid #000000"
                      />
                      <p style="margin: 0">{{chairperson_name}}</p>
                      <p style="margin: 0"><strong>CHAIRPERSON</strong></p>
                      <p style="margin: 0">
                        (Signature above printed full name)
                      </p>
                      <p style="margin: 0">{{chairperson_digital_signature}}</p>
                    </div>
                  </td>
                </tr>
                <!-- END Signature simple section -->
                <!-- Signature simple section -->
                <tr>
                  <td align="center" style="margin-top: 24px">
                    <div style="margin-top: 24px">
                      <hr
                        style="margin: 0 120px; border-top: 1px solid #000000"
                      />
                      <p style="margin: 0">{{poll_clerk_name}}</p>
                      <p style="margin: 0"><strong>POLL CLERK</strong></p>
                      <p style="margin: 0">
                        (Signature above printed full name)
                      </p>
                      <p style="margin: 0">{{poll_clerk_digital_signature}}</p>
                    </div>
                  </td>
                </tr>
                <!-- END Signature simple section -->
                <!-- Signature simple section -->
                <tr>
                  <td align="center" style="margin-top: 24px">
                    <div style="margin-top: 24px">
                      <hr
                        style="margin: 0 120px; border-top: 1px solid #000000"
                      />
                      <p style="margin: 0">{{third_member_name}}</p>
                      <p style="margin: 0"><strong>THIRD MEMEBER</strong></p>
                      <p style="margin: 0">
                        (Signature above printed full name)
                      </p>
                      <p style="margin: 0">
                        {{third_member_digital_signature}}
                      </p>
                    </div>
                  </td>
                </tr>
                <!-- END Signature simple section -->

                <!-- Title secondary section -->
                <tr>
                  <td align="center" style="padding: 15px">
                    <div style="text-align: center; padding: 15px">
                      <div
                        id="qrcode"
                        style="
                          width: 100%;
                          display: flex;
                          flex-direction: column;
                          justify-content: center;
                          align-items: center;
                        "
                      ></div>
                    </div>
                  </td>
                </tr>
                <!-- END Title secondary section -->

                <!-- Left paragraph section -->
                <tr>
                  <td align="center" style="padding: 10px">
                    <div style="text-align: left; margin-top: 24px">
                      Report hash: {{report_hash}}<br />
                      Software version: {{software_version}}<br />
                      OVCS version: {{ovcs_version}}<br />
                      System hash: {{system_hash}}
                    </div>
                  </td>
                </tr>
                <!-- END Left paragraph section -->

                <!-- End of report section -->
                <tr>
                  <td align="center">
                    <div style="text-align:center; margin: 16px; 0">
                      <hr
                        style="margin: 0 24px; border-top: 3px solid #000000"
                      />
                      <p>END OF REPORT</p>
                    </div>
                  </td>
                </tr>
                <!-- End of report section -->
              </table>
              <!-- Inner table section -->
            </div>
          </td>
        </tr>
      </table>
      <!-- MAIN section -->
</div>

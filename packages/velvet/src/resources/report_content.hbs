  <div class="content">
      <h1 class="title">Results of the election</h1>
      {{ #each reports as |report| }}
        <h2>Contest: {{{ report.contest.name }}}</h2>
        <p class="contest-desc">{{{ report.contest.description }}}</p>

        <table>
          <tr>
            <th colspan="2">Configuration Data</th>
          </tr>
          <tr>
            <td style="width: 55%;">Tally System</td>
            <td style="width: 45%;">
              {{ report.contest.voting_type }},
              {{ report.contest.counting_algorithm }}
            </td>
          </tr>
          <tr>
            <td style="width: 55%;">Minimum number of option a voter can select</td>
            <td style="width: 45%;">{{ format_u64 report.contest.min_votes }}</td>
          </tr>
          <tr>
            <td style="width: 55%;">Maximum number of option a voter can select</td>
            <td style="width: 45%;">{{ format_u64 report.contest.max_votes }}</td>
          </tr>
        </table>

        <table>
          <tr>
            <th colspan="2">Participation</th>
          </tr>
          <tr>
            <td style="width: 55%;">Total number of votes cast</td>
            <td style="width: 45%;">{{ format_u64 report.contest_result.total_votes }}</td>
          </tr>
          <tr>
            <td style="width: 55%;">Census</td>
            <td style="width: 45%;">{{ format_u64 report.contest_result.census }}</td>
          </tr>
          <tr>
            <td style="width: 55%;">Blank votes (total)</td>
            <td style="width: 45%;">{{ format_u64 report.contest_result.total_blank_votes }}</td>
          </tr>
          <tr>
            <td style="width: 55%;">Explicit invalid votes</td>
            <td style="width: 45%;">{{ format_u64 report.contest_result.invalid_votes.explicit }}</td>
          </tr>
          <tr>
            <td style="width: 55%;">Implicit invalid votes</td>
            <td style="width: 45%;">{{ format_u64 report.contest_result.invalid_votes.implicit }}</td>
          </tr>
          <tr>
            <td style="width: 55%;">Total number of valid votes</td>
            <td style="width: 45%;">{{ format_u64 report.contest_result.total_valid_votes }}</td>
          </tr>
        </table>

        <table>
          <tr>
            <th colspan="4">Candidate result</th>
          </tr>
          <tr class="">
            <td style="width: 55%;">Name</td>
            <td style="width: 15%;">Votes</td>
            <td style="width: 15%;">Percent of votes</td>
            <td style="width: 15%;">Winning Position</td>
          </tr>
          {{#each report.candidate_result as |candidate|}}
            <tr class="">
              <td style="width: 55%;">{{{ candidate.candidate.name }}}</td>
              <td style="width: 15%;">{{ format_u64 candidate.total_count }}</td>
              <td style="width: 15%;">{{ candidate.percentage_votes }}%</td>
              <td style="width: 15%;">{{ candidate.winning_position }}</td>
            </tr>
          {{/each}}
        </table>
      {{/each}}

    </div>

  "proxy": "https://i.imgur.com",
  "workspaces": [
    "ui-core",
    "ui-essentials",
    "voting-portal",
    "admin-portal",
    "ballot-verifier",
    "basic"
  ],
  "scripts": {
    "start:basic": "yarn --cwd ./basic start",
    "start:voting-portal": "yarn --cwd ./voting-portal start",
    "start:admin-portal": "yarn --cwd ./admin-portal start",
    "start:ballot-verifier": "yarn --cwd ./ballot-verifier start",
    "storybook:ui-essentials": "yarn --cwd ./ui-essentials storybook",
    "build:ballot-verifier": "yarn --cwd ./ballot-verifier build",
    "build:voting-portal": "yarn --cwd ./voting-portal build",
    "build:admin-portal": "yarn --cwd ./admin-portal build",
    "build:ui-essentials": "yarn --cwd ./ui-essentials build",
    "build:ui-core": "yarn --cwd ./ui-core build",
    "lint:fix:voting-portal": "yarn --cwd ./voting-portal lint:fix",
    "lint:fix:admin-portal": "yarn --cwd ./admin-portal lint:fix",
    "lint:fix:ballot-verifier": "yarn --cwd ./ballot-verifier lint:fix",
    "lint:fix:ui-essentials": "yarn --cwd ./ui-essentials lint:fix",
    "lint:fix:ui-core": "yarn --cwd ./ui-core lint:fix",
    "lint:fix": "yarn --cwd ./voting-portal lint:fix && yarn --cwd ./admin-portal lint:fix && yarn --cwd ./ballot-verifier lint:fix && yarn --cwd ./ui-essentials lint:fix",
    "lint": "yarn --cwd ./voting-portal lint && yarn --cwd ./admin-portal lint && yarn --cwd ./ballot-verifier lint && yarn --cwd ./ui-essentials lint && yarn --cwd ./ui-core lint",
    "prettify:fix:voting-portal": "yarn --cwd ./voting-portal prettify:fix",
    "prettify:fix:admin-portal": "yarn --cwd ./admin-portal prettify:fix",
    "prettify:fix:ballot-verifier": "yarn --cwd ./ballot-verifier prettify:fix",
    "prettify:fix:ui-essentials": "yarn --cwd ./ui-essentials prettify:fix",
    "prettify:fix:ui-core": "yarn --cwd ./ui-core prettify:fix",
    "prettify:fix": "yarn --cwd ./voting-portal prettify:fix && yarn --cwd ./admin-portal prettify:fix && yarn --cwd ./ballot-verifier prettify:fix && yarn --cwd ./ui-essentials prettify:fix && yarn --cwd ./ui-core prettify:fix",
    "prettify": "yarn --cwd ./voting-portal prettify && yarn --cwd ./admin-portal prettify && yarn --cwd ./ballot-verifier prettify && yarn --cwd ./ui-essentials prettify && yarn --cwd ./ui-core prettify",
    "generate:voting-portal": "yarn --cwd ./voting-portal generate",
    "generate:admin-portal": "yarn --cwd ./admin-portal generate",
    "generate:ballot-verifier": "yarn --cwd ./ballot-verifier generate",
    "generate:graphql-docs": "yarn --cwd ./api-docs/graphql generate-docs"
  }
}

{
  "private": true,
  "name": "sequentech",
  "version": "0.0.1",
  "license": "MIT",
  "proxy": "https://i.imgur.com",
  "workspaces": [
    "ui-essentials",
    "voting-portal",
    "admin-portal",
    "ballot-verifier"
  ],
  "scripts": {
    "start:voting-portal": "yarn --cwd ./voting-portal start",
    "start:admin-portal": "yarn --cwd ./admin-portal start",
    "start:ballot-verifier": "yarn --cwd ./ballot-verifier start",
    "storybook:ui-essentials": "yarn --cwd ./ui-essentials storybook",
    "build": "yarn workspaces run build",
    "build:ui-essentials": "yarn --cwd ./ui-essentials build",
    "lint:fix": "yarn --cwd ./voting-portal lint:fix",
    "prettify:fix": "yarn --cwd ./voting-portal prettify:fix",
    "generate:voting-portal": "yarn --cwd ./voting-portal generate",
    "generate:admin": "yarn --cwd ./admin-portal generate"
  }
}

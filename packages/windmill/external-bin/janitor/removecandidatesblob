#!/usr/bin/awk -f

# This script removes the BLOB data from INSERT INTO statements for the `candidate` table

# Match the INSERT INTO candidate statement
/INSERT INTO `candidate` VALUES/ {
    # This regex pattern matches the structure of your data
    # The pattern captures groups of data before and after the BLOB and replaces the BLOB with an empty string ''
    gsub(/(0x[0-9A-F]+),/, "'',", $0)
    
    # Print the cleaned-up line
    print
    next
}

# For all other lines, print as is
{
    print
}
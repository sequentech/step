{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "logs: harvest",
            "command": "docker",
            "args": [
                "compose",
                "logs",
                "-f",
                "--tail",
                "200",
                "harvest",
            ],
            "options": {
                "env": {
                    "COMPOSE_PROJECT_NAME": "backend-services_devcontainer",
                    "COMPOSE_PROFILES": "base",
                }
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "dedicated",
                "showReuseMessage": true,
                "clear": false
            },
            "isBackground": true,
            "runOptions": {
                "runOn": "folderOpen"
            },
            "problemMatcher": []
        },

{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "docker-compose:logs",
            "command": "docker",
            "args": [
                "compose",
                "logs",
                "--timestamps",
                "-f"
            ],
            "options": {
                "env": {
                    "COMPOSE_PROJECT_NAME": "backend-services_devcontainer"
                }
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "dedicated",
                "showReuseMessage": true,
                "clear": false
            },
            "isBackground": true,
            "runOptions": {
                "runOn": "folderOpen"
            },
            "problemMatcher": []
        },
        {
            "label": "proxy",
            "command": "devenv",
            "args": [
                "shell",
                "nginx",
                "-e", "/workspaces/backend-services/.devcontainer/nginx/error.log",
                "-p", "/workspaces/backend-services/.devcontainer/nginx/",
            ],
            "options": {
                "env": {
                    "COMPOSE_PROJECT_NAME": "backend-services_devcontainer"
                }
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "dedicated",
                "showReuseMessage": true,
                "clear": false
            },
            "isBackground": true,
            "runOptions": {
                "runOn": "folderOpen"
            },
            "problemMatcher": []
        }
    ]
  }
  